<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FirstScreenController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="250.0" prefWidth="250.0">
         <children>
            <Label fx:id="loginTitle" layoutX="110.0" layoutY="14.0" text="Login" />
            <TextField fx:id="loginUserName" layoutX="50.0" layoutY="44.0" promptText="Username" />
            <TextField fx:id="loginPassword" layoutX="50.0" layoutY="87.0" promptText="Password" />
            <Button fx:id="loginSubmit" layoutX="98.0" layoutY="134.0" mnemonicParsing="false" onAction="#submitLogin" text="Submit" />
            <Label fx:id="loginLocation" layoutX="75.0" layoutY="227.0" text="Location" />
            <Label fx:id="timezoneLabel" layoutX="11.0" layoutY="227.0" prefHeight="17.0" prefWidth="54.0" text="Timezone:" />
         </children></AnchorPane>
   </children>
</GridPane>
